FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      UAdj;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    topBoundary
    {
        type            symmetry;
       
    }

    upStream
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

   downStream
    {
        type            zeroGradient;
      

    }

subBoundary
    {
        type            fixedValue;
        value           uniform (1 0 0);
        
    }
    frontAndBack
    {
        type            empty;
    }
bottomSymmetry 
    { 
        type            symmetryPlane; 
    } 
}

// ************************************************************************* //
