FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
    grad(p)         Gauss linear;
    grad(pAdj)	    Gauss linear;
}

divSchemes
{
    default         none;
    div(phiAdj,U)      Gauss linear;
    div(phi,UAdj)      Gauss linear;
    div(U)		Gauss linear;
    div(UAdj)		Gauss linear;

}

laplacianSchemes
{
    default         Gauss linear orthogonal;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         orthogonal;
}

fluxRequired
{
    default         no;
    pAdj               ;
}


// ************************************************************************* //
